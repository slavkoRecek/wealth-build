type BankAccount {
    id: ID!
    name: String!
    iban: String!
}

type Query {
    getBankAccounts: [BankAccount]!
    getBankAccountByID (id: ID): BankAccount
    getBankAccountByIban(iban: String): BankAccount
}

type Mutation {
    createBankAccount(name: String, iban: String!): BankAccount
}
